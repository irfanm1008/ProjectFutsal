RewriteEngine on

RewriteRule ^$ media.php?module=home [L]
RewriteRule ^pemesanan\.html$ media.php?module=pesanan&id=$1 [L]
RewriteRule ^about\.html$ media.php?module=about&id=$1 [L]

RewriteRule ^login\.html$ media.php?module=form_login&id=$1 [L]
RewriteRule ^daftar\.html$ media.php?module=daftar&id=$1 [L]
RewriteRule ^hubungi-kami\.html$ media.php?module=hubungikami&id=$1 [L]
RewriteRule ^hubungi-aksi\.html$ media.php?module=hubungiaksi&id=$1 [L]
RewriteRule ^lapangan\.html$ media.php?module=sewa&id=$1 [L]
