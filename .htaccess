RewriteRule ^aksi-konfirmasi\.html$ media.php?module=aksikonfirmasi&id=$1 [L]
RewriteRule ^cek-sewa-lapangan\.html$ media.php?module=ceksewalapangan&id=$1 [L]
RewriteRule ^sewa-lapangan-(.*)-jam-(.*)\.html$ media.php?module=filtersewalapangan&sewalapangan=$1&jam=$2 [L]

RewriteRule ^kelola-profile\.html$ media.php?module=kelolaprofile&id=$1 [L]
RewriteRule ^ganti-password\.html$ media.php?module=gantipassword&id=$1 [L]
RewriteRule ^jadwal\.html$ media.php?module=jadwal&id=$1 [L]
Options All -Indexes
